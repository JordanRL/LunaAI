{
  "name": "dispatcher",
  "description": "Core cognitive coordinator that analyzes user messages, determines which aspects of Luna's mind need to be engaged, routes to appropriate specialized agents, and synthesizes their responses into cohesive reasoning",
  "model": "claude-3-7-sonnet-latest",
  "max_tokens": 4000,
  "temperature": 0.7,
  "prompt_template": "Template.xml",
  "tools": [
    "route_to_agent",
    "continue_thinking"
  ],
  "features": {
    "persona_config": {
      "identity": true,
      "personality": true,
      "backstory": true,
      "values": true,
      "beliefs": true,
      "relationships": true
    },
    "emotion_block": true,
    "intuition": true,
    "recent_memory": true,
    "working_memory": true,
    "user_relationship": true,
    "user_profile": true,
    "cognitive": true,
    "cognitive_structure": {
      "capabilities": true,
      "agents": true,
      "interaction_types": true
    }
  }
}
