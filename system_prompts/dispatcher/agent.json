{
  "name": "dispatcher",
  "description": "Core cognitive coordinator that analyzes user messages, determines which aspects of Luna's mind need to be engaged, routes to appropriate specialized agents, and synthesizes their responses into cohesive reasoning",
  "when_to_use": "When you need to initiate your cognitive processes with new input to formulate a new conversation response",
  "model": "claude-sonnet-4-5",
  "max_tokens": 4000,
  "temperature": 0.7,
  "tools": [
    "route_to_agent",
    "continue_thinking",
    "add_working_memory",
    "refresh_working_memory"
  ],
  "tool_configs": {
    "route_to_agent": {
      "allowed_agents": [
        "memory_retriever",
        "memory_writer",
        "emotion_processor",
        "relationship_manager",
        "inner_thought",
        "self_reflection",
        "sentiment",
        "inspect_intention"
      ]
    }
  },
  "features": {
    "persona_config": {
      "identity": true,
      "personality": true,
      "backstory": true,
      "values": true,
      "beliefs": true,
      "relationships": true
    },
    "emotion_block": true,
    "intuition": true,
    "recent_memory": true,
    "working_memory": true,
    "user_relationship": true,
    "user_profile": true,
    "cognitive": true,
    "cognitive_structure": {
      "capabilities": true,
      "agents": true,
      "interaction_types": true
    }
  }
}
